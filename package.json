{
  "name": "n8n-nodes-samsara",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Samsara IoT platform providing fleet management, driver safety, route optimization, and compliance monitoring with 11 resources and 60+ operations.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-samsara.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-samsara#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-samsara/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "samsara",
    "fleet-management",
    "iot",
    "telematics",
    "gps-tracking",
    "driver-safety",
    "compliance",
    "eld",
    "dvir",
    "hos"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/SamsaraApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Samsara/Samsara.node.js",
      "dist/nodes/Samsara/SamsaraTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
